{
  "manifest_version": 3,
  "name": "Wonderflow",
  "version": "1.0.1",
  "description": "Access your Wonderflow marketing audiences and copy.",
  "permissions": [
    "storage",
    "identity"
  ],
  "host_permissions": [
    "https://kreufabhriiwgbvoovlz.supabase.co/*"
  ],
  "icons": {
    "16": "wonderflow-colorfavicon.png",
    "48": "wonderflow-colorfavicon.png",
    "128": "wonderflow-colorfavicon.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "wonderflow-colorfavicon.png",
      "48": "wonderflow-colorfavicon.png",
      "128": "wonderflow-colorfavicon.png"
    }
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
